{
  "name": "react-playground",
  "version": "0.0.0-semantically-released",
  "description": "Playing with React",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "start": "node server.js",
    "start:prod": "cross-env NODE_ENV=production npm start",
    "cover": "babel-istanbul cover blue-tape src/{*,**/*}.spec.js",
    "cover:check": "babel-istanbul check",
    "test": "blue-tape spec/unit/{*,**/*}.spec.js | tap-dot",
    "docs": "jsdoc --pedantic -c .jsdoc . --readme README.md",
    "docs:check": "./ci/doc.sh",
    "lint": "eslint .",
    "build": "webpack",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obartra/react-playground.git"
  },
  "author": "Oscar Bartra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/obartra/react-playground/issues"
  },
  "homepage": "https://github.com/obartra/react-playground#readme",
  "dependencies": {
    "bootstrap": "4",
    "express": "4.14.1",
    "lodash": "4.17.4",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "@bubltechnology/customizable-commit-analyzer": "1.0.0",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-istanbul": "0.12.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-object-rest-spread": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2016": "6.22.0",
    "blue-tape": "1.0.0",
    "clean-webpack-plugin": "^0.1.15",
    "codeclimate-test-reporter": "0.4.0",
    "commitizen": "2.9.5",
    "cross-env": "3.1.4",
    "css-loader": "^0.26.1",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "2.28.0",
    "inchjs": "0.4.1",
    "jsdoc": "3.4.3",
    "json-loader": "0.5.4",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.2.2",
    "postcss-scss": "^0.4.0",
    "precss": "^1.4.0",
    "react-hot-loader": "3.0.0-beta.6",
    "semantic-release": "6.3.2",
    "semantic-release-cli": "3.0.3",
    "style-loader": "^0.13.1",
    "tap-dot": "1.0.5",
    "tape": "4.6.3",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-dev-server": "2.3.0",
    "webpack-hot-middleware": "2.16.1"
  },
  "release": {
    "verifyConditions": "condition-circle",
    "analyzeCommits": "@bubltechnology/customizable-commit-analyzer"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
